{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Wrangler",
			"type": "node",
			"request": "launch",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run",
				"dev:backend" // 我们将使用这个脚本来启动后端
			],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"sourceMaps": true,
			"skipFiles": ["<node_internals>/**"]
		},
	]
}
